<% if @evolution_move %>
  <%= render :partial => 'evolutions/if/current/move/all' %>
<% elsif @evolution_move_uni %>
  <%= render :partial => 'evolutions/if/current/move/one' %>
<% else %>
  <%= render :partial => 'evolutions/if/current/move/set' %>
<% end %>

<% if @evolution_clone %>
  <%= render :partial => 'evolutions/if/current/clone/all' %>
<% elsif @evolution_clone_uni %>
  <%= render :partial => 'evolutions/if/current/clone/one' %>
<% else %>
  <%= render :partial => 'evolutions/if/current/clone/set' %>
<% end %>
